{
    "name": "obsidian-markdown2html-plugin",
    "version": "1.2.1",
    "description": "saves markdown as html in the clipboard",
    "main": "main.js",
	"type": "module",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
    },
    "keywords": [
        "obsidian",
        "plugin",
        "converter"
    ],
    "author": "blotspot",
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "esbuild": "0.25.5",
        "eslint-plugin-obsidianmd": "0.1.9",
        "globals": "14.0.0",
        "tslib": "2.4.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "8.35.1",
        "@eslint/js": "9.30.1",
        "jiti": "2.6.1"
    },
    "dependencies": {
        "obsidian": "latest"
    }
}